    <input name="authenticity_token" value="<%= form_authenticity_token%>" type="hidden"></input>
    <input type="hidden" name="g_template_id" id="g_template_id" value="dfd"></input>
    <input type="hidden" name="scorms_ids" id="scorms_ids" value="dfdf"></input>
    <p>Create</p>
  </form>
</div>
</div>

