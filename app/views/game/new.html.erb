  <a id="upload_scorm" href="/scorm_file/new" ></a>
</div>

<div class="tool">
<img src="/assets/plus.png" class="plus" />

<div class="game">
  <div class="carrousel_title">
    <p class="carrousel_title_text"> choose game </p>
  </div>
  <div class="carrousel_wrapper">
    <div id="games_carrousel">
  </div>
  </div>
  <div class="data">
    <img id="game_preview"/>
    <p id="game_description"></p>
  </div>
</div>
